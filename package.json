{
	"name": "@sveltinio/ts-utils",
	"version": "0.6.0",
	"packageManager": "pnpm@8.4.0",
	"description": "A bunch of common utilities in pure Typescript to deal with primitives, strings, urls, objects, collections, dates etc.",
	"author": "sveltinio <dev@sveltin.io>",
	"license": "MIT",
	"homepage": "https://github.com/sveltinio/ts-utils#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sveltinio/ts-utils.git"
	},
	"bugs": "https://github.com/sveltinio/ts-utils/issues",
	"keywords": [
		"utilities",
		"helpers",
		"typescript",
		"strings",
		"urls",
		"collections",
		"dates"
	],
	"dependencies": {
		"neverthrow": "^6.0.0"
	},
	"devDependencies": {
		"@release-it/conventional-changelog": "^5.1.1",
		"@tsconfig/recommended": "^1.0.2",
		"@types/node": "^18.16.3",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"@vitest/coverage-c8": "^0.30.1",
		"c8": "^7.13.0",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-tsdoc": "^0.2.17",
		"prettier": "^2.8.8",
		"publint": "^0.1.11",
		"release-it": "^15.10.3",
		"shx": "^0.3.4",
		"tsup": "^6.7.0",
		"typedoc": "^0.24.6",
		"typedoc-plugin-coverage": "^2.1.0",
		"typedoc-plugin-extras": "^2.3.3",
		"typedoc-plugin-keywords": "^1.4.1",
		"typescript": "^5.0.4",
		"vite": "^4.3.4",
		"vitest": "^0.31.0"
	},
	"scripts": {
		"build": "pnpm prepare && pnpm test && tsup && pnpm postbuild",
		"watch": "pnpm build -- --watch src",
		"format": "prettier --plugin-search-dir=. --write .",
		"lint": "prettier --plugin-search-dir=. --check . && eslint .",
		"lint:fix": "prettier --plugin-search-dir=. --check . && eslint --fix .",
		"postbuild": "pnpm exec publint .",
		"prepare": "pnpm format && pnpm lint",
		"release": "pnpm exec release-it",
		"release:dryRun": "pnpm exec release-it --dry-run",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:watch": "vitest watch",
		"docs:generate": "npx typedoc && shx cp ./docs/coverage.svg .",
		"docs:watch": "npx typedoc --watch"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		"./collections": {
			"types": "./dist/collections/index.d.ts",
			"import": "./dist/collections/index.js",
			"require": "./dist/collections/index.cjs"
		},
		"./colors": {
			"types": "./dist/colors/index.d.ts",
			"import": "./dist/colors/index.js",
			"require": "./dist/colors/index.cjs"
		},
		"./dates": {
			"types": "./dist/dates/index.d.ts",
			"import": "./dist/dates/index.js",
			"require": "./dist/dates/index.cjs"
		},
		"./is": {
			"types": "./dist/is/index.d.ts",
			"import": "./dist/is/index.js",
			"require": "./dist/is/index.cjs"
		},
		"./objects": {
			"types": "./dist/objects/index.d.ts",
			"import": "./dist/objects/index.js",
			"require": "./dist/objects/index.cjs"
		},
		"./paths": {
			"types": "./dist/paths/index.d.ts",
			"import": "./dist/paths/index.js",
			"require": "./dist/paths/index.cjs"
		},
		"./strings": {
			"types": "./dist/strings/index.d.ts",
			"import": "./dist/strings/index.js",
			"require": "./dist/strings/index.cjs"
		},
		"./urls": {
			"types": "./dist/urls/index.d.ts",
			"import": "./dist/urls/index.js",
			"require": "./dist/urls/index.cjs"
		},
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"typesVersions": {
		">4.0": {
			"collections": [
				"./dist/collections/index.d.ts"
			],
			"colors": [
				"./dist/colors/index.d.ts"
			],
			"dates": [
				"./dist/dates/index.d.ts"
			],
			"is": [
				"./dist/is/index.d.ts"
			],
			"objects": [
				"./dist/objects/index.d.ts"
			],
			"paths": [
				"./dist/paths/index.d.ts"
			],
			"strings": [
				"./dist/strings/index.d.ts"
			],
			"urls": [
				"./dist/urls/index.d.ts"
			],
			"index": [
				"./dist/index.d.ts"
			]
		}
	}
}
